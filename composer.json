{
  "name": "jmvdevelop/graphql-generator",
  "description": "Generate a schema for webonyx/graphql-php",
  "authors": [
    {
      "name": "Joseph Maarek",
      "email": "josephmaarek@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "JmvDevelop\\GraphqlGenerator\\": "src"
    },
    "files": [
      "src/Utils/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "JmvDevelop\\GraphqlGenerator\\Example\\": "example"
    }
  },
  "bin": [
    "bin/graphql-generator"
  ],
  "require-dev": {
    "symfony/dependency-injection": "^5.3|^6.0",
    "symfony/config": "^5.3|^6.0",
    "symfony/finder": "^5.3|^6.0",
    "pestphp/pest": "^1.23",
    "spatie/pest-plugin-snapshots": "^1.1",
    "phpstan/phpstan": "^1.10",
    "php-standard-library/phpstan-extension": "^1.0"
  },
  "require": {
    "php": "^8.1",
    "symfony/console": "^4.0.0|^5.2|^6.0",
    "azjezz/psl": "^2.6",
    "league/flysystem-memory": "^3.15",
    "league/flysystem": "^3.15",
    "webonyx/graphql-php": "^15.5",
    "webmozart/assert": "^1.11",
    "nette/php-generator": "^4.0"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
