#!/usr/bin/env php
<?php

require_once __DIR__ . DIRECTORY_SEPARATOR . '..' . DIRECTORY_SEPARATOR . "vendor" . DIRECTORY_SEPARATOR . "autoload.php";

use JmvDevelop\GraphqlGenerator\Cli\GenerateCommand;
